import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int num=sc.nextInt();
        int rem=num%10;
        if(rem>=5)
        {
        num=num+10-rem;
            System.out.println(num);
        }
        else
        {
            num=num-rem;
            System.out.println(num);
        }
        
        
    }
}
